<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.virtualpet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NatureActivity" />
        <activity android:name=".DarkActivity" />
        <activity android:name=".EarthActivity" />
        <activity android:name=".LightActivity" />
        <activity android:name=".LightningActivity" />
        <activity android:name=".MagicActivity" />
        <activity android:name=".MetalActivity" />
        <activity android:name=".WaterActivity" />
        <activity android:name=".FireActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>